{
  "expo": {
    "name": "haiku-day",
    "slug": "haiku-day",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "owner": "daphaz",
    "scheme": "haikuday",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E3D7EA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#E3D7EA"
      },
      "useNextNotificationsApi": true,
      "userInterfaceStyle": "dark",
      "package": "com.daphaz.haikuday"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "description": "one haiku per day  in automation with schedule notification",
    "githubUrl": "https://github.com/Daphaz/haiku-day"
  }
}
